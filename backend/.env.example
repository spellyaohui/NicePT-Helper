# NicePT Helper 配置文件
# 复制此文件为 .env 并填写实际值

# 应用配置
SECRET_KEY=your-random-secret-key-here
DEBUG=true

# 数据库（默认 SQLite）
DATABASE_URL=sqlite+aiosqlite:///./nicept.db

# NicePT 站点配置
SITE_URL=https://your-nicept-site.com
SITE_PASSKEY=

# 请求配置（防风控）
REQUEST_TIMEOUT=30
REQUEST_DELAY=2.0
MAX_RETRIES=3

# 日志
LOG_LEVEL=INFO
